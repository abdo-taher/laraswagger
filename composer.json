{
    "name": "abdo-taher/laraswagger",
    "description": "Auto-generate Swagger-style API documentation for Laravel projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdo Taher",
            "email": "abdotaher093@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaraSwagger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraSwagger\\LaraSwaggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
