{
  "name": "abdo-taher/laraswagger",
  "description": "Automatic OpenAPI 3 & Swagger UI generator for Laravel",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LaraSwagger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaraSwagger\\LaraSwaggerServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  }
}
